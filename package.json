{
  "name": "vue-d",
  "version": "0.0.1",
  "scripts": {
    "serve": "vite preview",
    "prebuild": "eslint --fix src && prettier . --write && vue-tsc --noEmit -p tsconfig.vitest.json --composite false && vitest run --coverage",
    "build": "vite build",
    "format": "prettier . --write",
    "lint": "eslint src --ext .ts,.vue,.js --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "pinia": "^2.0.14",
    "roboto-fontface": "*",
    "three": "^0.141.x",
    "vue": "3.2.36",
    "vue-router": "^4.0.14",
    "webfontloader": "1.x.x"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.36",
    "@types/three": "^0.140.0",
    "@types/webfontloader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.x.x",
    "@vue/cli-plugin-eslint": "~5.0.x",
    "@vue/eslint-config-prettier": "^7.x.x",
    "@vue/eslint-config-typescript": "^10.x.x",
    "@vue/test-utils": "^2.x.x",
    "@vue/tsconfig": "^0.1.x",
    "c8": "^7.11.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "jsdom": "^19.0.0",
    "naive-ui": "^2.29.0",
    "prettier": "2.6.2",
    "typescript": "~4.6.3",
    "vfonts": "^0.0.3",
    "vite": "^2.9.x",
    "vitest": "^0.13.x",
    "vue-tsc": "0.35.x"
  }
}
